{
    "name": "Vector Addition",
    "description": ["This is a simple example of vector addition.The purpose of this code is to introduce the user to application development in the Vitis tools."],
    "flow": "vitis",
    "os": [
        "Linux"
    ],
    "runtime": [
        "OpenCL"
    ],
    "launch" : [
        {
            "cmd_args" : "BUILD/krnl_sobel_naive.xclbin"
        }
    ],
    "host": {
        "host_exe": "sobel_naive",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "src/sobel_naive.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    },     
    "containers" : [
        {
            "name" : "krnl_sobel_naive",
            "accelerators": [
                {
                    "name": "krnl_sobel_naive",
                    "location": "src/krnl_sobel_naive.cpp"
                }
            ]
        }
    ],
    "contributors" : [
        {
            "group" : "Xilinx, Inc.",
            "url" : "http://www.xilinx.com"
        }
    ],
    "revision" : [
        {
            "date" : "2019",
            "version" : "1.0"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
