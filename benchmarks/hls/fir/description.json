{
    "name": "FIR",
    "description": ["This is a simple example of vector addition.The purpose of this code is to introduce the user to application development in the Vitis tools."],
    "flow": "vitis",
    "os": [
        "Linux"
    ],
    "runtime": [
        "OpenCL"
    ],
    "platform_blacklist": [
        "nodma"
    ],
    "launch" : [
        {
            "cmd_args" : "BUILD/krnl_fir.xclbin"
        }
    ],
    "host": {
        "host_exe": "simple_fir",
        "compiler": {
            "sources": [
                "src/fir.cpp"
            ]
        }
    },     
    "containers" : [
        {
            "name" : "krnl_fir",
            "accelerators": [
                {
                    "name": "krnl_fir",
                    "location": "src/krnl_fir.cpp"
                }
            ]
        }
    ],
    "contributors" : [
        {
            "group" : "Xilinx, Inc.",
            "url" : "http://www.xilinx.com"
        }
    ],
    "revision" : [
        {
            "date" : "2019",
            "version" : "1.0"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
