[
  {
    "selector": "node",
    "css": {
      "background-fit": "contain",
      "border-color": "#000",
      "border-width": 1,
      "border-opacity": 0.5,
      "width": 50,
      "height": 50
    }
  },
  {
    "selector": ".selected",
    "css": {
      "background-color": "#90caf9",
      "border-color": "#002171",
      "line-color": "#002171",
      "target-arrow-color": "#002171"
    }
  },
  {
    "selector": ".eh-handle",
    "css": {
      "background-color": "red",
      "width": 15,
      "height": 15,
      "shape": "ellipse",
      "overlay-opacity": 0,
      "border-width": 3,
      "border-opacity": 0,
      "border-color": "#ab000d"
    }
  },
  {
    "selector": "edge",
    "css": {
      "curve-style": "bezier",
      "target-arrow-shape": "triangle"
    }
  },
  {
    "selector": ":selected",
    "css": {
      "background-color": "#90caf9",
      "border-color": "#002171"
    }
  },
  {
    "selector": ".reduce",
    "css": {
      "border-color": "#1b5e20",
      "border-width": 3
    }
  },
  {
    "selector": ".invalid",
    "css": {
      "background-color": "red",
      "line-color": "red",
      "target-arrow-color": "red",
      "source-arrow-color": "red",
      "border-color": "red",
      "border-width": 5
    }
  },
  {
    "selector": ".IOperator",
    "css": {
      "shape": "ellipse",
      "width": 70,
      "text-decoration": "underline"
    }
  },
  {
    "selector": "edge",
    "style": {
      "label": "data(port)",
      "text-background-color": "rgba(245, 245, 245)",
      "text-background-opacity": 0.5,
      "font-weight": "bold",
      "font-size": 20,
      "text-background-shape": "round-rectangle",
      "text-background-padding" : 5
    }
  }
]
